{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "accountType": {
      "type": "enumeration",
      "enum": [
        "condidate",
        "employer"
      ],
      "default": "condidate"
    },
    "fullname": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "age": {
      "type": "integer"
    },
    "birthday": {
      "type": "date"
    },
    "available": {
      "type": "boolean",
      "default": true
    },
    "company": {
      "autoPopulate": true,
      "via": "user",
      "model": "company"
    },
    "order": {
      "via": "user",
      "model": "order"
    },
    "subscribed": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "about": {
      "type": "text"
    },
    "postedJobs": {
      "via": "user",
      "collection": "job"
    },
    "title": {
      "type": "string",
      "default": "User"
    },
    "phone": {
      "type": "string",
      "unique": true
    },
    "country": {
      "type": "string"
    },
    "salary": {
      "type": "biginteger"
    },
    "skills": {
      "collection": "skill",
      "via": "users",
      "dominant": true
    },
    "website": {
      "type": "string"
    },
    "rate": {
      "type": "component",
      "repeatable": true,
      "component": "portfolio.rate"
    },
    "job_applieds": {
      "autoPopulate": true,
      "via": "applicants",
      "collection": "job",
      "dominant": true
    },
    "saved_jobs": {
      "collection": "job"
    },
    "notifications": {
      "type": "component",
      "repeatable": true,
      "component": "profile.notifications"
    },
    "appliesNbr": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "completedProjects": {
      "type": "integer"
    },
    "AcceptedInterviews": {
      "type": "integer"
    },
    "connects": {
      "type": "integer",
      "default": 30,
      "required": true
    },
    "avatar": {
      "type": "string"
    }
  }
}
